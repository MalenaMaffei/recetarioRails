<% if object && object.errors.size > 0 %>
  <div class="alert alert-danger">
      <h3>Solucionar los siguientes errores:</h3>
      <!-- <p>There were problems with the following fields:</p> -->
      <ul>
        <% object.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
  </div>
<% end %>
